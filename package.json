{
  "name": "rung",
  "version": "0.9.2",
  "description": "A randomness toolkit for browser-based game development, simulations and generative methods.",
  "repository": "git@github.com:maetl/rung",
  "author": "Mark Rickerby",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "compile": "tsc",
    "test": "ava --verbose"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "ava": "^4.1.0",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.2"
  },
  "module": "src/rung.js",
  "main": "rung.bundle.cjs",
  "browser": "src/rung.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/rung.js",
      "require": "./rung.bundle.cjs"
    },
    "./random": "./src/random.js",
    "./algorithms/": "./src/algorithms/"
  },
  "files": [
    "lib",
    "src",
    "README.md",
    "LICENSE",
    "rung.min.js"
  ]
}
